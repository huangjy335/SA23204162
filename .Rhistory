mybeta<-function(n,a,b,max_f=NULL){
n<-1e4
j<-0
y<-numeric(n)
while(j<n) {
u<-runif(1)
x<-runif(1) #random variate from g(.)
if(missing(max_f))
{
if(x^(a-1)*(1-x)^(b-1)>u){#pdf:f=x^(a-1)*(1-x)^(b-1)/B(a,b)
j<-j+1
y[j] <- x
}
}
else{
if(factorial(a+b-1)/(factorial(a-1)*factorial(b-1))*x^(a-1)*(1-x)^(b-1)/max_f>u){#pdf:f=x^(a-1)*(1-x)^(b-1)/B(a,b)
j<-j+1
y[j] <- x
}
}
}
return(y)
}
#使用默认c值的效果
hist(x=mybeta(1000,3,2),prob=TRUE,breaks=60,main=expression(f(x)==12*x^2*(1-x)),ylim=c(0,2))
